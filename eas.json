{
  "cli": {
    "version": ">= 3.0.0"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true, 
        "credentialsSource": "remote"
      }
    },
    "production": {
      "distribution": "store",
      "android": {
        "buildType": "apk" 
      },
      "ios": {
        "simulator": false,
        "credentialsSource": "remote",
        "autoIncrement": true
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false,
        "credentialsSource": "remote",
        "autoIncrement": true
      }
    },
    "internal": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false,
        "credentialsSource": "remote",
        "autoIncrement": true
      }
    }
  }
}